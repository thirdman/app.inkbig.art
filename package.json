{
  "name": "artbient-admin",
  "version": "0.2.0",
  "description": "Admin for vector iamges",
  "homepage": "https://app.inkbig.app",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "dev": "webpack-dev-server",
    "clean": "rimraf dist",
    "build": "cross-env npm run clean && webpack --config webpack.production.config.js --progress --profile --colors",
    "production": "cross-env npm run clean && webpack --config webpack.production.config.js --progress --profile --colors",
    "lint": "eslint --fix ./app/components/**/**.js",
    "lintmain": "eslint --fix ./app/**/*.js",
    "prettier": "prettier --write app/components/**/*.{js,jsx,ts,tsx}",
    "prettiermain": "prettier --write app/**/*.{js,jsx,ts,tsx}",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "precommit": "cross-env npm run lint && npm test",
    "prepush": "cross-env npm run lint && npm test"
  },
  "author": "Gareth Bedford",
  "license": "MIT",
  "jest": {
    "mapCoverage": true,
    "collectCoverageFrom": [
      "app/**/*.{js,jsx,ts,tsx}"
    ],
    "testMatch": [
      "**/?(*.)(spec|test).js?(x)"
    ]
  },
  "dependencies": {
    "adobe-swatch-exchange": "0.0.0",
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.6",
    "cross-env": "5.0.1",
    "dom-to-image": "^2.5.2",
    "firebase": "^4.5.1",
    "fsevents": "^2.0.1",
    "matrioshka.colors": "^1.0.28",
    "raw-loader": "^0.5.1",
    "rc-slider": "^8.3.1",
    "re-base": "^3.0.5",
    "react": "^15.6.2",
    "react-color": "^2.13.8",
    "react-dom": "15.6.2",
    "react-dropzone": "^7.0.1",
    "react-hot-loader": "3.0.0-beta.6",
    "react-inline-css": "^2.3.1",
    "react-router-dom": "4.3.1",
    "react-select": "^2.1.2",
    "react-toggle": "^4.0.2",
    "react-window": "^1.2.4",
    "svg-react-loader": "^0.4.5",
    "typeface-montserrat": "0.0.54"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "7.1.2",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-boilerplate": "1.1.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-2": "6.24.1",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "^0.28.7",
    "enzyme": "2.9.1",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-loader": "1.8.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "html-webpack-plugin": "2.30.0",
    "husky": "0.14.3",
    "jest": "21.1.0",
    "node-sass": "^4.10.0",
    "open-browser-webpack-plugin": "0.0.5",
    "prettier": "^1.15.2",
    "prettier-eslint": "^8.8.2",
    "react-addons-test-utils": "15.6.0",
    "react-test-renderer": "^15.6.1",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.6",
    "style-loader": "0.18.2",
    "url-loader": "0.5.8",
    "webpack": "3.6.0",
    "webpack-dev-server": "2.9.1"
  }
}
